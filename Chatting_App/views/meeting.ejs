</body>
<script>
          function copyLink() {
            const linkInput = document.querySelector('#Meetinglink');
            console.log(linkInput.innerHTML);
            navigator.clipboard.writeText(linkInput.innerHTML)
            document.execCommand('copy');
            console.log("copied");
        }
        const btnToCopyLink=document.querySelector(".copybtn");
        btnToCopyLink.addEventListener("click",()=>{
          copyLink();
        })
</script>

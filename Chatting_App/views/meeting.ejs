                <div class="top-meeting-section-00">
                    <div class="pt-5 pl-5 pr-5 ">
                        <div>
                          <h1 class="font-bold">Schedule Meeting</h1>
                        
                        </div>
                        <div class="w-full border-2 bg-gray-900"></div>
                        <div class="rounded-lg mt-2 w-full h-[250px] sm:h-[80px] bg-gray-200">
                          <div>
                            <form class="text-black p-4 flex justify-between align-middle flex-col sm:flex-row" action="/meeting/addmeeting" method="post">
                              <div class="flex flex-col sm:flex-row my-4">
                              <label for="schedule_date">Date : </label>
                              <input type="Date" id="schedule_date" name="date" required>
                              <label for="">Time : </label>
                              <input type="time" id="schedule_time" name="time" required>
                              <label for="meeting_type">Type : </label>
                              <select name="meeting_type" id="meeting_type" required>
                                <option value="" selected disabled>Select</option>
                                <option value="urgent">Urgent!</option>
                                <option value="lecture">Lecture</option>
                                <option value="event">Event</option>
                                <option value="meeting">Meeting</option>
                                <option value="meetup">Meetup</option>
                              </select>
                              </div>
                              <button type="submit"class="border-2 border-black hover:bg-red-400 hover:text-white transition-all p-2 text-red-600 rounded-lg">Schedule Meeting</button>
                            </form>
                          </div>
                        </div>
                    </div>
                </div>
                <div class="middle-meeting-section-00">
                  <div class="p-5">
                    <div>
                      <h1 class="font-bold">Meeting You Schedule</h1>
                      <div class="w-full border-2 bg-gray-900"></div>
                      <div class="my_meeting_cards flex flex-row space-x-5 overflow-x-auto">
                        <%if(!urMeetings){%>
                          <div class="w-full text-center p-6 text-[20px]"> - - No Meetings - - </div>
                          <%}%>
                        
                        <%for(let i of urMeetings){%>
                          <%console.log(i)%>
                        <div class="min-w-[310px] rounded-xl bg-white bg-clip-border text-gray-700 shadow-md my-4">
                          
                          <div class="p-6 pb-0">
                            <i class="fa-solid fa-business-time mb-4 text-5xl h-12 w-12 text-red-500"></i>
                            <h5 class="mb-2 block font-sans text-xl font-semibold leading-snug tracking-normal text-blue-gray-900 antialiased">
                              <%=(i.agenda.charAt(0)).toUpperCase()+(i.agenda).slice(1)%>
                            </h5>
                            
                              <div class="font-normal leading-relaxed antialiased w-[240px] truncate">
                                  <span class="whitespace-nowrap">Time: <%=i.startAt%></span><br/>
                                  <span class="whitespace-nowrap">Type: <%=i.agenda%></span><br/>
                                  <span class="whitespace-nowrap">UserID: <span id="Meetinglink" class="hidden"><%=i.meetingID%></span><button class="copybtn border-2 border-red-300 p-2 rounded-lg">Copy Link</button></button></span><br/>
                                  <span>password:<%=i.password%></span>
                              </div>
                           
                          
                          </div>
                         
                          <div class="p-6 pt-0">
                            <a
                              class="!font-medium !text-red-900 !transition-colors hover:!text-red-500"
                              href="<%=i.meetingLink%>"
                            >
                              <button
                                class="flex select-none items-center gap-2 rounded-lg py-2 px-4 text-center align-middle font-sans text-xs font-bold uppercase text-red-500 transition-all hover:bg-red-500/10 active:bg-red-500/30 disabled:pointer-events-none disabled:opacity-50 disabled:shadow-none"
                                type="button"
                                data-ripple-dark="true"
                              >
                                Join
                                <svg
                                  xmlns="http://www.w3.org/2000/svg"
                                  fill="none"
                                  viewBox="0 0 24 24"
                                  stroke-width="2"
                                  stroke="currentColor"
                                  aria-hidden="true"
                                  class="h-4 w-4"
                                >
                                  <path
                                    stroke-linecap="round"
                                    stroke-linejoin="round"
                                    d="M17.25 8.25L21 12m0 0l-3.75 3.75M21 12H3"
                                  ></path>
                                </svg>
                              </button>
                            </a>
                          </div>
                        </div>
                        <% } %>
                        
                      </div>
                    </div>
                  </div>
                </div>
                <div  class="bottom-meeting-section-00">
